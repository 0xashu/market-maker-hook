{
  "name": "app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "deployalllocal": "forge build --force && yarn anvil && yarn deploy:anvil && cd /src/nextjsv2 && npm run dev",
    "anvil": "source .env && anvil --fork-url $ANVIL_FORK_URL --chain-id $ANVIL_CHAIN_ID --code-size-limit 30000 --init genesis.json",
    "build": "cd src/nextjsv2 && npm run wagmi && next build",
    "deploy": "source .env && forge create contracts/src/Counter.sol:Counter --rpc-url $FORGE_RPC_URL --private-key $FORGE_PRIVATE_KEY",
    "deploy:anvil": "source .env && POOL_MANAGER_ADDR=0x565506C573abFE24Eb6abb7c0D8C809aCe1f638D forge script contracts/script/DeployHook.s.sol --broadcast --rpc-url http://127.0.0.1:8545 --private-key 0xac0974bec39a17e36ba4a6b4d238ff944bacb478cbed5efcae784d7bf4f2ff80",
    "deploy:arbitrum-goerli": "source .env && POOL_MANAGER_ADDR=0x4B8c70cF3e595D963cD4A33627d4Ba2718fD706F forge script contracts/script/DeployHook.s.sol --broadcast --rpc-url https://arbitrum-goerli.publicnode.com --private-key $FORGE_PRIVATE_KEY",
    "deploy:base-goerli": "source .env && POOL_MANAGER_ADDR=0x693B1C9fBb10bA64F0d97AE042Ee32aE9Eb5610D forge script contracts/script/DeployHook.s.sol --broadcast --rpc-url https://base-goerli.publicnode.com --private-key $FORGE_PRIVATE_KEY",
    "deploy:scroll-sepolia": "source .env && POOL_MANAGER_ADDR=0xA449635FaAA6b5a45a568fCe217Bb7921c992285 forge script contracts/script/DeployHook.s.sol --broadcast --rpc-url https://rpc.ankr.com/scroll_sepolia_testnet --private-key $FORGE_PRIVATE_KEY",
    "deploy:sepolia": "source .env && POOL_MANAGER_ADDR=0x64255ed21366DB43d89736EE48928b890A84E2Cb forge script contracts/script/DeployHook.s.sol --broadcast --rpc-url https://ethereum-sepolia.publicnode.com --private-key $FORGE_PRIVATE_KEY",
    "deploy:conduit": "source .env && POOL_MANAGER_ADDR=0xDc64a140Aa3E981100a9becA4E685f962f0cF6C9 forge script contracts/script/DeployHook.s.sol --broadcast --rpc-url https://l2-uniswap-v4-hook-sandbox-6tl5qq8i4d.t.conduit.xyz/ --private-key $FORGE_PRIVATE_KEY",
    "genesis": "source .env && forge script contracts/script/GenerateAnvilGenesis.s.sol --code-size-limit 30000 --ffi --broadcast --rpc-url http://127.0.0.1:8545 --private-key 0xac0974bec39a17e36ba4a6b4d238ff944bacb478cbed5efcae784d7bf4f2ff80",
    "dev": "cd src/nextjsv2 && next dev",
    "dev:foundry": "npm run dev & wagmi generate --watch & npm run anvil",
    "start": "next start",
    "lint": "next lint",
    "wagmi": "cd src/nextjsv2 && wagmi generate"
  },
  "dependencies": {
    "@ethersproject/providers": "^5.7.2",
    "@heroicons/react": "^2.0.11",
    "@nextui-org/react": "^2.1.13",
    "@rainbow-me/rainbowkit": "1.0.8",
    "@uniswap/sdk-core": "^4.0.1",
    "@uniswap/v2-sdk": "^3.0.1",
    "blo": "^1.0.1",
    "daisyui": "^3.5.1",
    "framer-motion": "^10.16.4",
    "next": "^13.3",
    "nextjs-progressbar": "^0.0.16",
    "qrcode.react": "^3.1.0",
    "react": "^18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-fast-marquee": "^1.3.5",
    "react-hot-toast": "^2.4.0",
    "react-icons": "^4.11.0",
    "react-tooltip": "^5.21.6",
    "use-debounce": "^8.0.4",
    "usehooks-ts": "^2.7.2",
    "viem": "^1.6.7",
    "wagmi": "1.3.10",
    "zustand": "^4.1.2"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/node": "20.8.5",
    "@types/react": "18.2.28",
    "@types/react-copy-to-clipboard": "^5.0.4",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@wagmi/cli": "^1.5.2",
    "autoprefixer": "^10.4.12",
    "eslint": "^8.15.0",
    "eslint-config-next": "^13.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "postcss": "^8.4.16",
    "prettier": "^2.8.4",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6",
    "vercel": "^28.15.1"
  }
}
